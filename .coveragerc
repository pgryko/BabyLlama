[run]
source = .
omit = 
    tests/*
    */__pycache__/*
    .venv/*
    venv/*
    */site-packages/*
    */distutils/*
    */tests/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod